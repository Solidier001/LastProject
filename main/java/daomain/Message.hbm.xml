<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="daomain">
    <class name="Message" table="Message">
        <id name="id" column="id">
            <generator class="native"></generator>
        </id>
       <property name="message" column="message" type="java.lang.String"/>
       <property name="form" column="form" type="java.lang.String"/>
       <property name="to" column="ot" type="java.lang.String"/>
       <property name="date" column="date" type="java.util.Date"/>
       <property name="name" column="name" type="java.lang.String"/>
    </class>
</hibernate-mapping>