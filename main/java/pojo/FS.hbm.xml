<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pojo.FS" table="FS">
        <id type="java.lang.String" name="id" column="id">
            <generator class="uuid"/>
        </id>
        <property name="faculty" type="java.lang.String" column="faculty"></property>
        <property name="specialty" type="java.lang.String" column="specialty"></property>
    </class>
</hibernate-mapping>